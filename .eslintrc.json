{
  "extends": "next/core-web-vitals",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "es2021": true
  },

  "rules" : {
    "no-unused-vars": "warn",
    "react/no-unescaped-entities": "off",
    "react/prop-types": "off"

  }
}
